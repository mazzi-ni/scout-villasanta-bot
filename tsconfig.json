{
  "compilerOptions": {
    "target": "es2016",                                 
    "lib": ["es6"],   
    "module": "commonjs",               
    "rootDir": "netlify",     
    "resolveJsonModule": true,          
    "allowJs": true,
    "outDir": "build",   
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "typeRoots": ["./node_modules/@types"],
    "noImplicitAny": true,
   "skipLibCheck": true                   
  }
}
